{
  "name": "boilerplate-redux-async",
  "version": "0.1.2",
  "description": "React Redux with simple async action",
  "main": "server/index",
  "scripts": {
    "dev": "run-p dev:server dev:client",
    "dev:client": "webpack --watch",
    "dev:server": "nodemon --watch server",
    "test": "jest --watchAll",
    "start": "node server",
    "postinstall": "webpack"
  },
  "author": "EDA",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/client/setup.js"
    ]
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.1.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "superagent": "^5.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
